{
  "name": "directus-operation-slugify",
  "description": "A slugify operation for Directus Flows",
  "icon": "extension",
  "version": "0.1.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-operation",
    "slugify"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "operation",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "source": {
      "app": "src/app.ts",
      "api": "src/api.ts"
    },
    "host": "^10.10.0",
    "sandbox": {
      "enabled": true,
      "requestedScopes": {}
    }
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "11.0.1",
    "@types/node": "^20.11.26",
    "typescript": "^5.4.2",
    "vue": "^3.4.21"
  }
}
